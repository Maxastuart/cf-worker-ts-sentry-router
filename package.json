{
  "name": "cf-worker-ts-sentry-router",
  "version": "1.0.0",
  "description": "TypeScript Cloudflare Worker Router Template with Sentry & Webpack",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "dev": "NODE_ENV=development yarn build",
    "format": "prettier --write '**/*.{ts,js,css,json,md}'",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx,.ts,.tsx --fix .",
    "test:clean": "rimraf ./transpiled/src ./transpiled/test",
    "test": "yarn test:clean && yarn transpile && mocha --require source-map-support/register --recursive transpiled/test",
    "transpile": "tsc --project ./test"
  },
  "author": "maxastuart",
  "license": "MIT OR Apache-2.0",
  "devDependencies": {
    "@cloudflare/workers-types": "^2.0.0",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-sort-keys-fix": "^1.1.1",
    "chai": "^4.2.0",
    "mocha": "^8.0.1",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "service-worker-mock": "^2.0.5",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  }
}
